{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'github>rishid/renovate-config:default.json5',
  ],
  platform: 'github',
  packageRules: [
    {
      matchUpdateTypes: [
        'minor',
        'patch',
        'pin',
        'digest',
        'lockFileMaintenance',
      ],
      automerge: true,
    },
  ],
  onboarding: false,
  requireConfig: 'ignored',
}
